all: DEMOS

RDFS: LMATH32.RDF LMATH64.RDF

DEMOS: DEMO32.EXE DEMO64.EXE

DEMO32.EXE: DEMO32.PAS LMATH32.TPU
        TPC DEMO32.PAS

DEMO64.EXE: DEMO64.PAS LMATH32.TPU
        TPC DEMO64.PAS

LMATH32.TPU: LMATH32.PAS LMATH64.OBJ LMATH32.OBJ
        TPC LMATH32.PAS

LMATH32.OBJ: LMATH32.ASM
        NASM -f obj -DTURBO LMATH32.ASM

LMATH64.OBJ: LMATH64.ASM
        NASM -f obj -DTURBO LMATH64.ASM

LMATH32.RDF: LMATH32.ASM
        NASM -f rdf LMATH32.ASM

clean:
        del LMATH32.RDF
        del LMATH32.OBJ
        del LMATH64.RDF
        del LMATH64.OBJ
        del LMATH32.TPU
        del DEMO32.EXE
        del DEMO64.EXE
        del *.BAK

